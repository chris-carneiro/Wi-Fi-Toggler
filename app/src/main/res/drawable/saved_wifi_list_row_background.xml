<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- checked -->
    <item android:state_activated="true" android:drawable="@drawable/switch_banner_blue_gradient"/>
    <!--<item android:state_activated="true" android:drawable="@drawable/switch_banner_blue_gradient"/>-->
    <!-- focused -->
    <item android:drawable="@drawable/switch_banner_blue_gradient" android:state_checked="true"/>
    <item android:drawable="@color/background_floating_material_light" android:state_checked="false"/>
    <!-- default -->
    <item android:drawable="@color/background_floating_material_light"/>
</selector>